/* pages/home/home.wxss */
.swiperBox{
  width: 100%;
  height: 340rpx;
}
.swiperList{
  width: 100%;
  height: 100%;
}
.swiperBox .swiperList image{
  width: 100%;
  height: 100%;
}
/* 分类 */
.cateList{
  width: 100%;
  display: flex;
  margin-top: 20rpx;
}
.cateList .cate image {
  width: 100%;
  height: 100%;
}
.cateList .cate{
  width: 100rpx;
  height: 190rpx;
  padding: 20rpx;
  flex:1
}
/* 详情 */
.index_floor{
  width: 100%;
  display: flex;
  flex-direction : column;
}
.index_floor .floor_group{
  width: 100%;
  height: 80rpx;
  margin-bottom: 10rpx;
  margin: 20rpx 0;
}
.index_floor .floor_group image{
  width: 100%;
  height: 100%;
}
.floor_list{
  overflow: hidden;
}
.floor_list navigator{
  float: left;
  width: 33.33%;
}
.floor_list navigator:nth-child(1){
  height: 500rpx;
}
.floor_list navigator:nth-child(n+2){
  width: 33.33%;
  height: 250rpx;
  border-left: 10rpx solid #fff;
}
.floor_list navigator:nth-child(2),.floor_list navigator:nth-child(3){
  border-bottom: 10rpx solid #fff;
}
.floor_list navigator image{
  width: 100%;
  height: 100%;
}
